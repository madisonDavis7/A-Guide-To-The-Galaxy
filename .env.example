# Environment variables for Railway deployment
SECRET_KEY=your-secret-key-here
DEBUG=False
PYTHONUNBUFFERED=1

# Railway automatically provides DATABASE_URL, no need to set these:
# POSTGRES_DB, POSTGRES_USER, POSTGRES_PASSWORD, etc.

# Optional: Custom domain configuration
# ALLOWED_HOSTS=your-custom-domain.com

# Firebase Configuration (if using Firebase services)
FIREBASE_PROJECT_ID=fnof-stack-a31a1
FIREBASE_PRIVATE_KEY=your-firebase-private-key
FIREBASE_CLIENT_EMAIL=your-firebase-client-email

# Local development only
DJANGO_DATABASE=local
